<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id ="student1" class ="com.java.spring.Student">
		<constructor-arg>
			<value>홍길동</value>
		</constructor-arg>
		<constructor-arg>
			<value>15</value>
		</constructor-arg>
		<constructor-arg>
			<value>8</value>
		</constructor-arg>
		<constructor-arg>
			<value>3</value>
		</constructor-arg>
	</bean>
	
	<bean id ="student2" class = "com.java.spring.Student">
		<constructor-arg value ="홍길동"/>
		<constructor-arg value ="17"/>
		<constructor-arg value ="10"/>
		<constructor-arg value ="6"/>
	</bean>
	
	<bean id = "studentInfo" class ="com.java.spring.StudentInfo">
		<constructor-arg>
			<ref bean="student1"/>
		</constructor-arg>
	</bean>
	
	<bean id="pencil" class ="com.java.spring.Pencil4B">
	</bean>
</beans>	